do_install() {
	vinstall ${FILESDIR}/splash.png 644 usr/share/plan10-artwork
	vinstall ${FILESDIR}/splashwhite.png 644 usr/share/plan10-artwork
	vinstall ${FILESDIR}/plan10-logo.png 644 usr/share/plan10-artwork
	vinstall ${FILESDIR}/plan10-transparent.png 644 usr/share/plan10-artwork
}
